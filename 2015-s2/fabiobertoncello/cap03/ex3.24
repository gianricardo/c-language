


inteiro contador,numero,maior_numero,repeticoes=0;

	mostre(informe a quantidade de numeros)
	recebe(contador)

	mostre(informe o valor a ser testado)
	recebe(maior_numero)
enquanto(repeticoes<contador){
	mostre(informe o valor a ser testado)
	recebe(numero)
	se (numero>=maior_numero){
		maior_numero=numero
	}
	incrementa repeticoes

}

